{
  "name": "real-estates",
  "version": "1.0.0",
  "description": "An application where you can see real estate with the option to buy it",
  "homepage": "https://Alver23.github.io/real-estates",
  "dependencies": {
    "@craco/craco": "6.1.2",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "12.8.3",
    "@types/node": "12.20.13",
    "@types/react-dom": "17.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "4.3.2",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "start": "craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.tsx ./src --ignore-path .eslintignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -f html -o reports/lint.html",
    "prettier": "prettier --config .prettierrc --ignore-path ./.prettierignore --write src/**/*.{ts, tsx}",
    "release": "standard-version",
    "sonarqube": "gulp sonarqube",
    "stylelint": "stylelint './src/**/*.scss'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "craco test --watchAll=false",
    "test:watch": "craco test --verbose",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alver23/real-estates.git"
  },
  "keywords": [
    "NodeJs",
    "Typescript",
    "React",
    "Tailwindcss",
    "jest"
  ],
  "author": "Alver Grisales <viga.23@hotmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@testing-library/react-hooks": "7.0.0",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.1",
    "autoprefixer": "9.8.6",
    "babel-jest": "26.6.3",
    "coveralls": "3.1.0",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.2.0",
    "gulp": "4.0.2",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.0",
    "jest-html-reporters": "2.1.6",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "11.0.0",
    "node-sass": "6.0.0",
    "postcss": "7.0.35",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.3.0",
    "sonarqube-scanner": "2.8.0",
    "source-map-explorer": "2.5.2",
    "standard-version": "9.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.1.2",
    "ts-node": "10.0.0"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
